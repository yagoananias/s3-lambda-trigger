{ 
   "Version":"2012-10-17",
   "Statement":[ 
      { 
         "Effect":"Allow",
         "Action":[ 
            "s3:GetObject"
         ],
         "Resource":[ 
            "arn:aws:s3:::mysourcebucket12345/*"
         ]
      },
      { 
         "Effect":"Allow",
         "Action":[ 
            "s3:PutObject"
         ],
         "Resource":[ 
            "arn:aws:s3:::mydestinationbucket12345/*"
         ]
      }
   ]
}
